{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:/temp/wiser_api_logs/front-end/log.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "AllowedHosts": "*",
  "GCL": {
    "Environment": "live",
    "SecretsBaseDirectory": "C:\\WiserSecrets\\FrontEnd\\"
  },
  "FrontEnd": {
    "ApiBaseUrl": "https://api.wiser3.nl/",
    "WiserHostNames": [
      ".wiser.nl",
      ".wiser3.nl"
    ],
    "MainSubDomain": "main",
    "AllowedCorsOrigins": [
      "*.happyhorizon.review",
      "*.happyhorizon.dev",
      "smaakvol.hollandfoodservice.nl",
      "gamecardsdirect.com",
      "www.gamecardsdirect.com"
    ]
  }
}