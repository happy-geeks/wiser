@model FrontEnd.Core.Models.BaseViewModel
@inject FrontEnd.Core.Interfaces.IWebPackService WebPackService

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <meta charset="utf-8" />
    <title>Wiser 3 - Admin</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="//kendo.cdn.telerik.com/2022.2.621/styles/kendo.common-material.min.css" rel="stylesheet" />
    <link href="//kendo.cdn.telerik.com/2022.2.621/styles/kendo.material.min.css" rel="stylesheet" />
    <script>
        window.wiserVersion = "@Model.WiserVersion?.ToString()";
    </script>
    <style>
        #wiser, #MainTabStrip {
            display:  none;
        }
    </style>
</head>
<body data-module-id="706"
      data-wiser-api-root="@Model.ApiRoot"
      data-is-test-environment="@Model.IsTestEnvironment.ToString().ToLowerInvariant()"
      data-sub-domain="@Model.SubDomain"
      data-wiser-api-authentication-url="@Model.ApiAuthenticationUrl"
      data-api-client-id="@Model.Settings.ApiClientId"
      data-api-client-secret="@Model.Settings.ApiClientSecret"
      data-track-js-token="@Model.Settings.TrackJsToken">
    <div id="wiser">
        <div class="loaderWrap">
            <div class="lds-ellipsis">
                <div></div>
                <div></div>
                <div></div>
                <div></div>
            </div>
        </div>
        <div id="window" class="module-admin" style="margin: 0 auto; padding: 0;">
            <div class="pane-content">
                <!-- TABS -->
                <div class="tabstrip k-tabstrip-wrapper" id="MainTabStrip">
                    <ul>
                        <li class="k-state-active">Modules</li>
                        <li>Entiteiten</li>
                        <li>Rollen</li>
                        <li>Query's</li>
                        <li style="display:none">Entiteiten Genereren</li>
                    </ul>
                    <div class="tabSplit">
                        <div class="left-pane">
                            <div class="item">
                                <h4>
                                    <label>Module selecteren of toevoegen</label>
                                </h4>
                                <div class="item">
                                    <select id="moduleList"></select>
                                </div>
                                <div class="buttons">
                                    <span>
                                        <button class="k-primary addModuleBtn">Toevoegen</button>
                                        <button class="k-primary delModuleBtn hidden">Verwijderen</button>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="right-pane">
                            <div class="pane-content">
                                <div class="formview full">
                                    <div class="item" style="width: 25%;">
                                        <h4>
                                            <label>ID van de module</label>
                                        </h4>
                                        <span class="k-widget k-input k-state-default">
                                            <input id="moduleId" type="text" class="textField k-input" autocomplete="off" />
                                        </span>
                                    </div>
                                    <div class="item" style="width: 50%;">
                                        <h4>
                                            <label>Naam van de module</label>
                                        </h4>
                                        <span class="k-widget k-input k-state-default">
                                            <input id="moduleName" type="text" class="textField k-input" autocomplete="off" />
                                        </span>
                                    </div>
                                    <div class="item" style="width: 25%;">
                                        <h4>
                                            <label for="input-type">Icoon</label>
                                            <span class="info" data-type="moduleIcon"><ins class="icon-info-full"></ins></span>
                                        </h4>
                                        <select id="moduleIcon">
                                        </select>
                                    </div>
                                    <div class="item" style="width: 50%;">
                                        <h4>
                                            <label>Type</label>
                                        </h4>
                                        <select id="moduleType" placeholder="Maak uw keuze...">
                                        </select>
                                    </div>
                                    <div class="item" style="width: 50%;">
                                        <h4>
                                            <label>Groep</label>
                                        </h4>
                                        <select id="moduleGroup"></select>
                                    </div>
                                    <div class="item" style="width: 100%;">
                                        <h4>
                                            <label>Query</label>
                                        </h4>
                                        <textarea id="moduleCustomQuery"></textarea>
                                    </div>
                                    <div class="item" style="width: 100%;">
                                        <h4>
                                            <label>Query voor het aantal records</label>
                                        </h4>
                                        <textarea id="moduleCountQuery"></textarea>
                                    </div>
                                    <div class="item" style="width: 100%;">
                                        <h4>
                                            <label>Opties</label>
                                        </h4>
                                        <textarea id="moduleOptions"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div>
                        <div class="pane-content">
                            <div class="formview full">
                                <div class="item flex-container" id="entityView">
                                    <h4>
                                        <label>Entiteit selecteren of toevoegen</label>
                                    </h4>
                                    <div class="flex-container">
                                        <select id="entities"></select>
                                        <button class="k-primary addBtn" data-type="entity">Toevoegen</button>
                                        <button class="k-primary delBtn hidden" data-type="entity">Verwijderen</button>
                                    </div>
                                </div>
                                <div class="item">
                                    <div class="tabstrip k-tabstrip-wrapper" id="EntityTabStrip">
                                        <ul>
                                            <li class="k-state-active">Eigenschappen</li>
                                            <li>Velden</li>
                                        </ul>
                                        <div>
                                            <div class="pane-content flex-container">
                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>Naam</label>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <input id="entityName" type="text" class="textField k-input" autocomplete="off" />
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>Vriendelijke Naam</label>
                                                        <span class="info" data-type="friendlyName"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <input id="friendlyName" type="text" class="textField k-input" autocomplete="off" />
                                                    </span>
                                                </div>
                                            
                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>Tabelnaam voorvoegsel</label>
                                                        <span class="info" data-type="entityDedicatedTablePrefix"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <input id="entityDedicatedTablePrefix" type="text" class="textField k-input" autocomplete="off" />
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>Module</label>
                                                        <span class="info" data-type="entityModule"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityModule">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>Geaccepteerde entiteit(en)</label>
                                                        <span class="info" data-type="entityAcceptedChildTypes"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="acceptedChildTypes">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>Sortering</label>
                                                        <span class="info" data-type="defaultOrdering"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="defaultOrdering" placeholder="Maak uw keuze...">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label for="input-type">Icoon</label>
                                                        <span class="info" data-type="entityIcon"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityIcon">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label for="input-type">Icoon toevoegen</label>
                                                        <span class="info" data-type="entityIconAdd"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityIconAdd">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label for="input-type">Icoon uitgeklapt</label>
                                                        <span class="info" data-type="entityIconExpanded"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityIconExpanded">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>Kleur</label>
                                                        <span class="info" data-type="entityColor"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityColor" placeholder="Maak uw keuze...">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>Actie bij verwijderen</label>
                                                        <span class="info" data-type="deleteAction"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="deleteAction" placeholder="Maak uw keuze...">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showInTreeView" type="checkbox" />
                                                            <span>Tonen in tree view</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 10%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showInSearch" type="checkbox" />
                                                            <span>Tonen bij zoeken</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showInOverviewTab" type="checkbox" />
                                                            <span>Tonen in overzicht</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="saveTitleAsSEO" type="checkbox" />
                                                            <span>Altijd SEO-variant van titel opslaan</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="enableMultipleEnvironments" type="checkbox" />
                                                            <span>Meerdere omgevingen toestaan</span>
                                                        </label>
                                                        <span class="info" data-type="enableMultipleEnvironments"><ins class="icon-info-full"></ins></span>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 10%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showTitleField" type="checkbox" />
                                                            <span>Toon titel veld</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 10%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="saveHistory" type="checkbox" />
                                                            <span>Geschiedenis opslaan</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 10%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showInDashboard" type="checkbox" />
                                                            <span>Tonen in dashboard</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item half">
                                                    <h4>
                                                        <label>Query na toevoegen</label>
                                                        <span class="info" data-type="queryAfterInsert"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <textarea id="queryAfterInsert"></textarea>
                                                </div>

                                                <div class="item half">
                                                    <h4>
                                                        <label>Query voor verwijderen</label>
                                                        <span class="info" data-type="queryBeforeDelete"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <textarea id="queryBeforeDelete"></textarea>
                                                </div>

                                                <div class="item half">
                                                    <h4>
                                                        <label>Query voor het bijwerken</label>
                                                        <span class="info" data-type="queryBeforeUpdate"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <textarea id="queryBeforeUpdate"></textarea>
                                                </div>

                                                <div class="item half">
                                                    <h4>
                                                        <label>Query na bewerken</label>
                                                        <span class="info" data-type="queryAfterUpdate"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <textarea id="queryAfterUpdate"></textarea>
                                                </div>

                                                <div class="item half">
                                                    <h4>
                                                        <label>Template Query</label>
                                                        <span class="info" data-type="templateQuery"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <textarea id="templateQuery"></textarea>
                                                </div>

                                                <div class="item half">
                                                    <h4>
                                                        <label>Template HTML</label>
                                                        <span class="info" data-type="templateHtml"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <textarea id="templateHtml"></textarea>
                                                </div>
                                            </div>
                                        </div>

                                        <div>
                                            <div class="tabSplit staticSplit">
                                                <div class="left-pane">
                                                    <div class="pane-content">
                                                        <div class="item" id="entityPropertyView" style="display: none;">
                                                            <h4>
                                                                <label>Tabnaam</label>
                                                            </h4>
                                                            <select id="tabnames">
                                                            </select>
                                                            <div class="form-hint"><span>Om een nieuwe tab te maken; maak een nieuw veld aan, of wijzig een bestaand veld, en vul bij "Tab" de naam voor de nieuwe tab in.</span></div>
                                                            <ul id="tabNameProperties">
                                                            </ul>
                                                            <div class="form-hint"><span>Sleep items om de volgorde te veranderen</span></div>
                                                            <div class="buttons">
                                                                <span>
                                                                    <button class="k-primary addBtn" data-type="entityProperty">Toevoegen</button>
                                                                    <button class="k-primary delBtn" data-type="entityProperty">Verwijderen</button>
                                                                </span>
                                                            </div>
                                                            <div class="form-hint"><span>Klik op toevoegen om een veld toe te voegen aan de entiteit</span></div>
                                                            <div class="buttons">
                                                                <span>
                                                                    <button class="k-primary copyToOtherLanguagesButton" data-type="entityProperty">Kopiëren naar andere talen</button>
                                                                </span>
                                                            </div>
                                                            <div class="form-hint"><span>Hiermee kan dit veld gekopieerd worden naar alle talen die dit veld nog niet hebben.</span></div>
                                                            <div class="buttons">
                                                                <span>
                                                                    <button class="k-primary duplicateEntityPropertyButton" data-type="entityProperty">Veld dupliceren</button>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="right-pane">
                                                    <div class="pane-content flex-container">
                                                        <div class="item-group">
                                                            <h3>Algemene instellingen</h3>
                                                            <div class="item">
                                                                <h4>
                                                                    <label for="inputtypes">Invoertype</label>
                                                                    <span class="info" data-type="inputtype"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="inputtypes" class="inputTypeCombobox">
                                                                </select>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>Veld naam</label>
                                                                    <span class="info" data-type="displayName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="displayname" type="text" class="textField k-input" maxlength="100" autocomplete="off" />
                                                                </span>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>Eigenschap naam</label>
                                                                    <span class="info" data-type="propertyName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="propertyname" type="text" class="textField k-input" maxlength="100" autocomplete="off" />
                                                                </span>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>Tab</label>
                                                                    <span class="info" data-type="tabName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="tabNameProperty">
                                                                </select>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>Groep</label>
                                                                    <span class="info" data-type="groupName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="groupName"></select>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>Breedte (in %)</label>
                                                                    <span class="info" data-type="width"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <input id="width" type="number" class="numeric" value="" min="0" max="100" step="1" autocomplete="off" />
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>Hoogte (in pixels)</label>
                                                                    <span class="info" data-type="height"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <input id="height" type="number" class="numeric" value="" max="65535" min="0" step="1" autocomplete="off" />
                                                            </div>
                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>Taalcode</label>
                                                                    <span class="info" data-type="languageCode"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="langCode" type="text" class="textField k-input" maxlength="5" autocomplete="off" />
                                                                </span>
                                                            </div>
                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>Sneltoets (access key)</label>
                                                                    <span class="info" data-type="accessKey"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="accessKey" type="text" class="textField k-input" maxlength="1" autocomplete="off" />
                                                                </span>
                                                            </div>
                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>Labelstijl</label>
                                                                    <span class="info" data-type="labelStyle"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <select id="labelStyle"></select>
                                                                </span>
                                                            </div>
                                                            <div class="item" id="labelWidthContainer" style="width: 25%;">
                                                                <h4>
                                                                    <label>Breedte label</label>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <select id="labelWidth"></select>
                                                                </span>
                                                            </div>
                                                        
                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>Regex voor pad</label>
                                                                    <span class="info" data-type="visibilityPathRegex"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="visibilityPathRegex" type="text" class="textField k-input" max="255" autocomplete="off" />
                                                                </span>
                                                            </div>

                                                            <div class="item" style="width: 50%;" data-visible="Input SecureInput TextBox GpsLocation HtmlEditor ColorPicker ComboBox DataSelector DateTimePicker MultiSelect Qr Empty">
                                                                <h4>
                                                                    <label>Standaardwaarde</label>
                                                                    <span class="info" data-type="defaultValue"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="defaultValue" type="text" class="textField k-input" autocomplete="off" value="" />
                                                                </span>
                                                            </div>

                                                            <div class="item" style="width: 50%" data-visible="NumericInput AutoIcrement">
                                                                <h4>
                                                                    <label>Standaardwaarde</label>
                                                                    <span class="info" data-type="defaultValue"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="defaultNumeric" type="number" class="numeric" value="" min="0" step="1" autocomplete="off" />
                                                                </span>
                                                            </div>

                                                            <div class="item" style="width: 50%" data-visible="RadioButton CheckBox">
                                                                <h4>
                                                                    <label>Standaardwaarde</label>
                                                                    <span class="info" data-type="defaultValue"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="checkedCheckbox" placeholder="Maak uw keuze...">
                                                                    <option value="0" selected>Uitgevinkt</option>
                                                                    <option value="1">Aangevinkt</option>
                                                                </select>
                                                            </div>
                                                            
                                                            <div class="item" data-invisible="FileUpload ImageUpload ColorPicker DataSelector SubEntitiesGrid">
                                                                <div class="toggle">
                                                                    <input id="visible-in-table" type="checkbox" class="noForm">
                                                                    <label class="checkbox" for="visible-in-table">
                                                                        <span>Standaard zichtbaar in overzichtstabellen</span>
                                                                    </label>
                                                                    <div class="togglePanel">
                                                                        <div class="item" style="width: 50%;">
                                                                            <h4>
                                                                                <label>Breedte in tabel (in pixels)</label>
                                                                                <span class="info" data-type="overviewWidth"><ins class="icon-info-full"></ins></span>
                                                                            </h4>
                                                                            <input id="widthIfVisible" type="number" class="numeric" min="0" step="1" max="65535" autocomplete="off" />
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>

                                                            <div class="item" data-invisible="FileUpload ImageUpload DataSelector SubEntitiesGrid">
                                                                <div class="toggle">
                                                                    <input id="enableAggregation" type="checkbox" class="noForm">
                                                                    <label class="checkbox" for="enableAggregation">
                                                                        <span>Aggregatie inschakelen</span>
                                                                    </label>
                                                                    <div class="togglePanel">
                                                                        <div class="item" style="width: 60%;">
                                                                            <h4>
                                                                                <label>Aggregatie-instellingen</label>
                                                                                <span class="info" data-type="aggregateOptions"><ins class="icon-info-full"></ins></span>
                                                                            </h4>
                                                                            <textarea id="aggregateOptions"></textarea>
                                                                        </div>
                                                                        <div class="item" style="width: 40%;">
                                                                            <h4>
                                                                                <label>Voorbeeld</label>
                                                                            </h4>
                                                                            <pre class="aggregate-example">
{
	"tableName": "aggregate_test",
	"columnName": "test_custom_column",
	"aggregationMethods": [
		{
			"method": "Sum",
			"parentLinkType": 1338
		},
		{
			"method": "Average",
			"parentLinkType": 1338
		}
	]
}
</pre>
                                                                        </div>
                                                                    </div>
                                                                </div>
