@model FrontEnd.Core.Models.BaseViewModel

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <meta charset="utf-8" />
    <title>Wiser 3 - Admin</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="//kendo.cdn.telerik.com/2021.2.511/styles/kendo.common-material.min.css" rel="stylesheet" />
    <link href="//kendo.cdn.telerik.com/2021.2.511/styles/kendo.material.min.css" rel="stylesheet" />
    <script>
        window.wiserVersion = "@Model.WiserVersion?.ToString()";
    </script>
    <style>
        #wiser, #MainTabStrip {
            display:  none;
        }
    </style>
</head>
<body data-module-id="706"
      data-wiser-api-root="@Model.ApiRoot"
      data-is-test-environment="@Model.IsTestEnvironment.ToString().ToLowerInvariant()"
      data-sub-domain="@Model.SubDomain">
    <div id="wiser">
        <div class="loaderWrap">
            <div class="lds-ellipsis">
                <div></div>
                <div></div>
                <div></div>
                <div></div>
            </div>
        </div>
        <div id="window" class="module-admin" style="margin: 0 auto; padding: 0;">
            <div class="pane-content">
                <!-- TABS -->
                <div class="tabstrip" id="MainTabStrip">
                    <ul>
                        <li style="display:none">Entiteiten Genereren</li>
                        <li class="k-state-active">Entiteiten</li>
                        <li style="display:none">Modules</li>
                        <li>Rollen</li>
                        <li>Query's</li>
                        <li>Modules</li>
                    </ul>
                    <div>
                        <div class="formview full">
                            <div class="item" id="generateEntitiesGroup">
                                <span class="handler">&nbsp;</span>
                                <span class="inline">
                                    <label class="checkbox">
                                        <input id="generateEcommerceEntity" data-entity-group="ecommerce" data-entity-pretty-name="Ë-commerce" type="checkbox" />
                                        <span>E-commerce entiteiten</span>
                                    </label>

                                    <label class="checkbox">
                                        <input id="generateExtra" data-entity-group="extra" data-entity-pretty-name="Extra" type="checkbox" />
                                        <span>Extra entiteiten</span>
                                    </label>
                                </span>
                            </div>
                            <div class="item">
                                <span class="handler">&nbsp;</span>

                                <span class="k-widget k-input k-state-default">
                                    <button type="button" id="generateStandardEntities">Genereer standaard entiteiten</button>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div>
                        <div class="pane-content">
                            <div class="formview full">
                                <div class="item flex-container" id="entityView">
                                    <h4>
                                        <label>Entiteit selecteren of toevoegen</label>
                                    </h4>
                                    <div class="flex-container">
                                        <select id="entities"></select>
                                        <button class="k-primary addBtn" data-type="entity" style="width: 25%; margin: 4px">Toevoegen</button>
                                    </div>
                                </div>
                                <div class="item">
                                    <div class="tabstrip" id="EntityTabStrip">
                                        <ul>
                                            <li class="k-state-active">Eigenschappen</li>
                                            <li>Velden</li>
                                        </ul>
                                        <div>
                                            <div class="pane-content flex-container">
                                                <div class="item" style="width: 25%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <h4>
                                                        <label>Naam</label>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <input id="entityName" type="text" class="textField k-input" autocomplete="off" />
                                                    </span>
                                                </div>
												<div class="item" style="width: 25%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <h4>
                                                        <label>Tabelnaam voorvoegsel </label>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <input id="entityDedicatedTablePrefix" type="text" class="textField k-input" autocomplete="off" />
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 50%;">
                                                    <span class="handler">&nbsp;</span>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <h4>
                                                        <label>Module</label>
                                                        <span class="info" data-type="entityModule"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityModule">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <h4>
                                                        <label>Geaccepteerde entiteit(en)</label>
                                                        <span class="info" data-type="entityAcceptedChildtypes"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="acceptedChildTypes">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <h4>
                                                        <label>Vriendelijke Naam</label>
                                                        <span class="info" data-type="friendlyName"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <input id="friendlyName" type="text" class="textField k-input" autocomplete="off" />
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <h4>
                                                        <label>Sortering</label>
                                                        <span class="info" data-type="defaultOrdering"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="defaultOrdering" placeholder="Maak uw keuze...">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <h4>
                                                        <label for="input-type">Icoon</label>
                                                        <span class="info" data-type="entityIcon"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityIcon">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <h4>
                                                        <label for="input-type">Icoon toevoegen</label>
                                                        <span class="info" data-type="entityIconAdd"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityIconAdd">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <h4>
                                                        <label for="input-type">Icoon uitgeklapt</label>
                                                        <span class="info" data-type="entityIconExpanded"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityIconExpanded">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <h4>
                                                        <label>Kleur</label>
                                                        <span class="info" data-type="entityColor"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityColor" placeholder="Maak uw keuze...">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showInTreeView" type="checkbox" />
                                                            <span>Tonen in tree view</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showInSearch" type="checkbox" />
                                                            <span>Tonen bij zoeken</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showInOverviewTab" type="checkbox" />
                                                            <span>Tonen in overzicht</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="saveTitleAsSEO" type="checkbox" />
                                                            <span>SEO titel opslaan</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showTitleField" type="checkbox" />
                                                            <span>Toon titel veld</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="handler">&nbsp;</span>
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="saveHistory" type="checkbox" />
                                                            <span>Geschiedenis opslaan</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 10%;">
                                                    <span class="handler">&nbsp;</span>
                                                </div>

                                                <div class="item">
                                                    <h4>
                                                        <label>Query na toevoegen</label>
                                                        <span class="info" data-type="queryAfterInsert"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <textarea id="queryAfterInsert"></textarea>
                                                    </span>
                                                </div>

                                                <div class="item">
                                                    <h4>
                                                        <label>Query na bewerken</label>
                                                        <span class="info" data-type="queryAfterUpdate"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <textarea id="queryAfterUpdate"></textarea>
                                                    </span>
                                                </div>

                                                <div class="item">
                                                    <h4>
                                                        <label>Query voor het bijwerken</label>
                                                        <span class="info" data-type="queryBeforeUpdate"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <textarea id="queryBeforeUpdate"></textarea>
                                                    </span>
                                                </div>

                                                <div class="item">
                                                    <h4>
                                                        <label>Query na verwijderen</label>
                                                        <span class="info" data-type="queryBeforeDelete"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <textarea id="queryBeforeDelete"></textarea>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>

                                        <div>
                                            <div class="tabSplit staticSplit">
                                                <div class="left-pane">
                                                    <div class="pane-content">
                                                        <div class="item" id="entityPropertyView" style="display: none;">
                                                            <h4>
                                                                <label>Tabnaam</label>
                                                            </h4>
                                                            <select id="tabnames">
                                                            </select>
                                                            <div class="form-hint"><span>Om een nieuwe tab te maken; maak een nieuw veld aan, of wijzig een bestaand veld, en vul bij "Tab" de naam voor de nieuwe tab in.</span></div>
                                                            <ul id="tabNameProperties">
                                                            </ul>
                                                            <div class="form-hint"><span>Sleep items om de volgorde te veranderen</span></div>
                                                            <div class="buttons">
                                                                <span>
                                                                    <button class="k-primary addBtn" data-type="entityProperty">Toevoegen</button>
                                                                    <button class="k-primary delBtn" data-type="entityProperty">Verwijderen</button>
                                                                </span>
                                                            </div>
                                                            <div class="form-hint"><span>Klik op toevoegen om een veld toe te voegen aan de entiteit</span></div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="right-pane">
                                                    <div class="pane-content flex-container">
                                                        <div class="pane-content flex-container">
                                                            <div class="item">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label for="input-type">Invoertype</label>
                                                                    <span class="info" data-type="inputtype"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="inputtypes" class="inputTypeCombobox">
                                                                </select>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Veld naam</label>
                                                                    <span class="info" data-type="displayName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="displayname" type="text" class="textField k-input" maxlength="100" autocomplete="off" />
                                                                </span>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Eigenschap naam</label>
                                                                    <span class="info" data-type="propertyName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="propertyname" type="text" class="textField k-input" maxlength="100" autocomplete="off" />
                                                                </span>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Tab</label>
                                                                    <span class="info" data-type="tabName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="tabNameProperty">
                                                                </select>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Groep</label>
                                                                    <span class="info" data-type="groupName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="groupName"></select>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Breedte (in %)</label>
                                                                    <span class="info" data-type="width"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <input id="width" type="number" title="numeric" class="numeric" value="" min="0" max="100" step="1" autocomplete="off" />
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Hoogte (in pixels)</label>
                                                                    <span class="info" data-type="height"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <input id="height" type="number" title="numeric" class="numeric" value="" max="65535" min="0" step="1" autocomplete="off" />
                                                            </div>
                                                            <div class="item" style="width: 25%;">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Taalcode</label>
                                                                    <span class="info" data-type="languageCode"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="langCode" type="text" class="textField k-input" maxlength="5" autocomplete="off" />
                                                                </span>
                                                            </div>
                                                            <div class="item" style="width: 25%;">
                                                                &nbsp;
                                                            </div>
                                                            <div class="item" style="width: 100%;" data-visible="input numeric-input secure-input textbox gpslocation">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Regex validatie</label>
                                                                    <span class="info" data-type="regexValidation"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="regexValidation" type="text" class="textField k-input" max="255" autocomplete="off" />
                                                                </span>
                                                            </div>
                                                            <div class="item" data-invisible="file-upload image-upload color-picker data-selector">
                                                                <span class="handler">&nbsp;</span>
                                                                <div class="toggle">
                                                                    <input id="visible-in-table" type="checkbox" class="noForm">
                                                                    <label class="checkbox" for="visible-in-table">
                                                                        <span>Standaard zichtbaar in overzichtstabellen</span>
                                                                    </label>
                                                                    <div class="togglePanel">
                                                                        <div class="item" style="width: 50%;">
                                                                            <h4>
                                                                                <label>Breedte in tabel (in pixels)</label>
                                                                                <span class="info" data-type="overviewWidth"><ins class="icon-info-full"></ins></span>
                                                                            </h4>
                                                                            <input id="widthIfVisible" type="number" title="numeric" class="numeric" value="" min="0" step="1" max="65535" autocomplete="off" />
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="item">
                                                                <span class="handler">&nbsp;</span>
                                                                <span class="inline">
                                                                    <label class="checkbox" data-invisible="radiobutton checkbox empty">
                                                                        <input id="mandatory" type="checkbox">
                                                                        <span>Verplicht veld</span>
                                                                    </label>
                                                                    <label class="checkbox" data-invisible="radiobutton checkbox file-upload image-upload color-picker data-selector empty">
                                                                        <input id="readonly" type="checkbox">
                                                                        <span>Alleen lezen veld</span>
                                                                    </label>
                                                                    <label class="checkbox" data-visible="input">
                                                                        <input id="seofriendly" type="checkbox">
                                                                        <span>Waarde van dit veld ook opslaan als zoekmachine-vriendelijke tekst</span>
                                                                    </label>
                                                                    <label class="checkbox" data-visible="combobox">
                                                                        <input id="useDropDownList" type="checkbox">
                                                                        <span>Combobox als dropdown gebruiken</span>
                                                                    </label>
                                                                    <label class="checkbox" data-show-for-panel="panel2">
                                                                        <input id="searchInTitle" type="checkbox">
                                                                        <span>Zoeken in titel</span>
                                                                    </label>
                                                                    <label class="checkbox" data-show-for-panel="panel2">
                                                                        <input id="searchEverywhere" type="checkbox">
                                                                        <span>Toon alle gekoppelde items</span>
                                                                    </label>
                                                                </span>
                                                            </div>

                                                            <div class="item tagList" data-show-for-panel="panel2">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Zoek in velden</label>
                                                                    <span class="info" data-type="searchFields"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <select id="searchFields"></select>
                                                                </span>
                                                            </div>

                                                            <div class="item" data-visible="textbox">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Textbox type</label>
                                                                    <span class="info" data-type="textboxType"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="textboxTypeDropDown" />
                                                                </span>
                                                            </div>

                                                            <div class="item" data-visible="input secure-input textbox gpslocation">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Standaard waarde</label>
                                                                    <span class="info" data-type="defaultValue"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="defaultValue" type="text" class="textField k-input" autocomplete="off" value="" />
                                                                </span>
                                                            </div>

                                                            <div class="item" data-visible="numeric-input auto-increment">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Standaard waarde</label>
                                                                    <span class="info" data-type="defaultValue"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="defaultNumeric" type="number" title="numeric" class="numeric" value="" min="0" step="1" autocomplete="off" />
                                                                </span>
                                                            </div>

                                                            <div class="item" data-visible="radiobutton checkbox">
                                                                <span class="handler">&nbsp;</span>
                                                                <h4>
                                                                    <label>Standaard waarde</label>
                                                                    <span class="info" data-type="defaultValue"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="checkedCheckbox" placeholder="Maak uw keuze...">
                                                                    <option value="1" selected>Aangevinkt</option>
                                                                    <option value="0">Uitgevinkt</option>
                                                                </select>
                                                            </div>

                                                            <div class="item datetime" data-visible="date-time picker">
                                                                <span class="handler">&nbsp;</span>

                                                                <div class="item" style="width: 50%;">
                                                                    <h4>
                                                                        <label>Mode</label>
                                                                        <span class="info" data-type="date-time picker mode"><ins class="icon-info-full"></ins></span>
                                                                    </h4>
                                                                    <select id="dateTimeDropDown">
                                                                    </select>
                                                                </div>

                                                                <div data-invisible="time" class="item" style="width: 25%;">
                                                                    <h4>
                                                                        <label>Huidige datum</label>
                                                                    </h4>
                                                                    <span class="inline">
                                                                        <label class="checkbox">
                                                                            <input type="checkbox" id="dateTimePickerSetNow" value="current-date">
                                                                            <span>Huidige datum als default invullen</span>
                                                                        </label>
                                                                    </span>
                                                                </div>

                                                                <div data-invisible="date" class="item" style="width: 25%;">
                                                                    <h4>
                                                                        <label>Minimale tijd</label>
                                                                    </h4>
                                                                    <input id="minimumTime" />
                                                                </div>
                                                            </div>

                                                            <div class="item" data-visible="numeric-input">
                                                                <span class="handler">&nbsp;</span>

                                                                <div class="item" style="width: 10%;">
                                                                    <span class="inline">
                                                                        <label class="checkbox">
                                                                            <input type="checkbox" id="roundNumeric" checked="checked">
                                                                            <span>Afronden</span>
                                                                        </label>
                                                                    </span>
                                                                </div>
                                                                <div class="item" style="width: 10%;">
                                                                    <h4>
                                                                        <label>Aantal decimalen</label>
                                                                    </h4>
                                                                    <input id="numberOfDecimals" type="number" title="numeric" class="numeric" value="" min="0" max="65" step="1" autocomplete="off" />
                                                                </div>

                                                                <div class="item" style="width: 10%;">
                                                                    <h4>
                                                                        <label>Maximale waarde</label>
                                                                    </h4>
                                                                    <input id="maxNumber" type="number" class="numeric" value="" min="0" max="99999" step="1" autocomplete="off" />
                                                                </div>

                                                                <div class="item" style="width: 10%;">
                                                                    <h4>
                                                                        <label>Minimale waarde</label>
                                                                    </h4>
                                                                    <input id="minNumber" type="number" class="numeric" value="" max="99999" step="1" autocomplete="off" />
                                                                </div>

                                                                <div class="item" style="width: 10%;">
                                                                    <h4>
                                                                        <label>Stapwaarde</label>
                                                                    </h4>
                                                                    <input id="stepNumber" type="number" class="numeric" value="" max="99999" autocomplete="off" />
                                                                </div>

                                                                <div class="item" style="width: 10%;">
                                                                    <h4>
