@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@model FrontEnd.Core.Models.BaseViewModel
@inject FrontEnd.Core.Interfaces.IWebPackService WebPackService

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <meta charset="utf-8" />
    <title>@Localizer["Wiser 3 - Admin"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="//kendo.cdn.telerik.com/2022.2.621/styles/kendo.common-material.min.css" rel="stylesheet" />
    <link href="//kendo.cdn.telerik.com/2022.2.621/styles/kendo.material.min.css" rel="stylesheet" />
    <script>
        window.wiserVersion = "@Model.WiserVersion?.ToString()";
    </script>
    <style>
        #wiser, #MainTabStrip {
            display:  none;
        }
    </style>
</head>
<body data-module-id="706"
      data-wiser-api-root="@Model.ApiRoot"
      data-is-test-environment="@Model.IsTestEnvironment.ToString().ToLowerInvariant()"
      data-sub-domain="@Model.SubDomain"
      data-wiser-api-authentication-url="@Model.ApiAuthenticationUrl"
      data-api-client-id="@Model.Settings.ApiClientId"
      data-api-client-secret="@Model.Settings.ApiClientSecret"
      data-track-js-token="@Model.Settings.TrackJsToken">
    <div id="wiser">
        <div class="loaderWrap">
            <div class="lds-ellipsis">
                <div></div>
                <div></div>
                <div></div>
                <div></div>
            </div>
        </div>
        <div id="window" class="module-admin" style="margin: 0 auto; padding: 0;">
            <div class="pane-content">
                <!-- TABS -->
                <div class="tabstrip k-tabstrip-wrapper" id="MainTabStrip">
                    <ul>
                        <li class="k-state-active">@Localizer["Modules"]</li>
                        <li>@Localizer["Entities"]</li>
                        <li>@Localizer["Roles"]</li>
                        <li>@Localizer["Query's"]</li>
                        <li style="display:none">@Localizer["Generate Entities"]</li>
                    </ul>
                    <div class="tabSplit">
                        <div class="left-pane">
                            <div class="item">
                                <h4>
                                    <label>@Localizer["Select or add module"]</label>
                                </h4>
                                <div class="item">
                                    <select id="moduleList"></select>
                                </div>
                                <div class="buttons">
                                    <span>
                                        <button class="k-primary addModuleBtn">@Localizer["Add"]</button>
                                        <button class="k-primary delModuleBtn" style="display: none">@Localizer["Delete"]</button>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="right-pane">
                            <div class="pane-content">
                                <div class="formview full">
                                    <div class="item" style="width: 25%;">
                                        <h4>
                                            <label>@Localizer["Module ID"]</label>
                                        </h4>
                                        <span class="k-widget k-input k-state-default">
                                            <input id="moduleId" type="text" class="textField k-input" autocomplete="off" />
                                        </span>
                                    </div>
                                    <div class="item" style="width: 50%;">
                                        <h4>
                                            <label>@Localizer["Module name"]</label>
                                        </h4>
                                        <span class="k-widget k-input k-state-default">
                                            <input id="moduleName" type="text" class="textField k-input" autocomplete="off" />
                                        </span>
                                    </div>
                                    <div class="item" style="width: 25%;">
                                        <h4>
                                            <label for="input-type">@Localizer["Icon"]</label>
                                            <span class="info" data-type="moduleIcon"><ins class="icon-info-full"></ins></span>
                                        </h4>
                                        <select id="moduleIcon">
                                        </select>
                                    </div>
                                    <div class="item" style="width: 50%;">
                                        <h4>
                                            <label>@Localizer["Type"]</label>
                                        </h4>
                                        <select id="moduleType" placeholder="@Localizer["Make your choice..."]">
                                        </select>
                                    </div>
                                    <div class="item" style="width: 50%;">
                                        <h4>
                                            <label>@Localizer["Group"]</label>
                                        </h4>
                                        <select id="moduleGroup"></select>
                                    </div>
                                    <div class="item" style="width: 100%;">
                                        <h4>
                                            <label>@Localizer["Query"]</label>
                                        </h4>
                                        <textarea id="moduleCustomQuery"></textarea>
                                    </div>
                                    <div class="item" style="width: 100%;">
                                        <h4>
                                            <label>@Localizer["Query for the number of records"]</label>
                                        </h4>
                                        <textarea id="moduleCountQuery"></textarea>
                                    </div>
                                    <div class="item" style="width: 100%;">
                                        <h4>
                                            <label>@Localizer["Options"]</label>
                                        </h4>
                                        <textarea id="moduleOptions"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div>
                        <div class="pane-content">
                            <div class="formview full">
                                <div class="item flex-container" id="entityView">
                                    <h4>
                                        <label>@Localizer["Select or add entity"]</label>
                                    </h4>
                                    <div class="flex-container">
                                        <select id="entities"></select>
                                        <button class="k-primary addBtn" data-type="entity" style="width: 25%; margin: 4px">@Localizer["Add"]</button>
                                    </div>
                                </div>
                                <div class="item">
                                    <div class="tabstrip k-tabstrip-wrapper" id="EntityTabStrip">
                                        <ul>
                                            <li class="k-state-active">@Localizer["Properties"]</li>
                                            <li>@Localizer["Fields"]</li>
                                        </ul>
                                        <div>
                                            <div class="pane-content flex-container">
                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>@Localizer["Name"]</label>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <input id="entityName" type="text" class="textField k-input" autocomplete="off" />
                                                    </span>
                                                </div>
												<div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>@Localizer["Table name prefix"] </label>
                                                        <span class="info" data-type="entityDedicatedTablePrefix"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <input id="entityDedicatedTablePrefix" type="text" class="textField k-input" autocomplete="off" />
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 50%;">
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>@Localizer["Module"]</label>
                                                        <span class="info" data-type="entityModule"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityModule">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>@Localizer["Accepted entity(ies)"]</label>
                                                        <span class="info" data-type="entityAcceptedChildTypes"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="acceptedChildTypes">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>@Localizer["Friendly Name"]</label>
                                                        <span class="info" data-type="friendlyName"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <span class="k-widget k-input k-state-default">
                                                        <input id="friendlyName" type="text" class="textField k-input" autocomplete="off" />
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>@Localizer["Sorting"]</label>
                                                        <span class="info" data-type="defaultOrdering"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="defaultOrdering" placeholder="@Localizer["Make your choice..."]">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label for="input-type">@Localizer["Icon"]</label>
                                                        <span class="info" data-type="entityIcon"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityIcon">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label for="input-type">@Localizer["Add icon"]</label>
                                                        <span class="info" data-type="entityIconAdd"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityIconAdd">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label for="input-type">@Localizer["Icon expanded"]</label>
                                                        <span class="info" data-type="entityIconExpanded"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityIconExpanded">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 25%;">
                                                    <h4>
                                                        <label>@Localizer["Color"]</label>
                                                        <span class="info" data-type="entityColor"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <select id="entityColor" placeholder="@Localizer["Make your choice..."]">
                                                    </select>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showInTreeView" type="checkbox" />
                                                            <span>@Localizer["Show in tree view"]</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showInSearch" type="checkbox" />
                                                            <span>@Localizer["Show in search"]</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showInOverviewTab" type="checkbox" />
                                                            <span>@Localizer["Show in overview tab"]</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="saveTitleAsSEO" type="checkbox" />
                                                            <span>@Localizer["Save title as SEO"]</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="showTitleField" type="checkbox" />
                                                            <span>@Localizer["Show title field"]</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 15%;">
                                                    <span class="inline">
                                                        <label class="checkbox">
                                                            <input id="saveHistory" type="checkbox" />
                                                            <span>@Localizer["Save history"]</span>
                                                        </label>
                                                    </span>
                                                </div>

                                                <div class="item" style="width: 10%;">
                                                </div>

                                                <div class="item">
                                                    <h4>
                                                        <label>@Localizer["Query after insert"]</label>
                                                        <span class="info" data-type="queryAfterInsert"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <textarea id="queryAfterInsert"></textarea>
                                                </div>

                                                <div class="item">
                                                    <h4>
                                                        <label>@Localizer["Query after update"]</label>
                                                        <span class="info" data-type="queryAfterUpdate"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <textarea id="queryAfterUpdate"></textarea>
                                                </div>

                                                <div class="item">
                                                    <h4>
                                                        <label>@Localizer["Query before update"]</label>
                                                        <span class="info" data-type="queryBeforeUpdate"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <textarea id="queryBeforeUpdate"></textarea>
                                                </div>

                                                <div class="item">
                                                    <h4>
                                                        <label>@Localizer["Query before delete"]</label>
                                                        <span class="info" data-type="queryBeforeDelete"><ins class="icon-info-full"></ins></span>
                                                    </h4>
                                                    <textarea id="queryBeforeDelete"></textarea>
                                                </div>
                                            </div>
                                        </div>

                                        <div>
                                            <div class="tabSplit staticSplit">
                                                <div class="left-pane">
                                                    <div class="pane-content">
                                                        <div class="item" id="entityPropertyView" style="display: none;">
                                                            <h4>
                                                                <label>@Localizer["Tabname"]</label>
                                                            </h4>
                                                            <select id="tabnames">
                                                            </select>
                                                            <div class="form-hint"><span>@Localizer["To create a new tab; create a new field, or change an existing field, and enter the name for the new tab at 'Tab'"]</span></div>
                                                            <ul id="tabNameProperties">
                                                            </ul>
                                                            <div class="form-hint"><span>@Localizer["Drag items to change the order"]</span></div>
                                                            <div class="buttons">
                                                                <span>
                                                                    <button class="k-primary addBtn" data-type="entityProperty">@Localizer["Add"]</button>
                                                                    <button class="k-primary delBtn" data-type="entityProperty">@Localizer["Delete"]</button>
                                                                </span>
                                                            </div>
                                                            <div class="form-hint"><span>@Localizer["Click 'Add' to add a field to the entity"]</span></div>
                                                            <div class="buttons">
                                                                <span>
                                                                    <button class="k-primary copyToOtherLanguagesButton" data-type="entityProperty">@Localizer["Copy to other languages"]</button>
                                                                </span>
                                                            </div>
                                                            <div class="form-hint"><span>@Localizer["This allows rhis field to be copied to all languages that do not yet have this field."]</span></div>
                                                            <div class="buttons">
                                                                <span>
                                                                    <button class="k-primary duplicateEntityPropertyButton" data-type="entityProperty">@Localizer["Duplicate field"]</button>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="right-pane">
                                                    <div class="pane-content flex-container">
                                                        <div class="pane-content flex-container">
                                                            <div class="item">
                                                                <h4>
                                                                    <label for="input-type">@Localizer["InputType"]</label>
                                                                    <span class="info" data-type="inputtype"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="inputtypes" class="inputTypeCombobox">
                                                                </select>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>@Localizer["Field name"]</label>
                                                                    <span class="info" data-type="displayName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="displayname" type="text" class="textField k-input" maxlength="100" autocomplete="off" />
                                                                </span>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>@Localizer["Property name"]</label>
                                                                    <span class="info" data-type="propertyName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="propertyname" type="text" class="textField k-input" maxlength="100" autocomplete="off" />
                                                                </span>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>@Localizer["Tab"]</label>
                                                                    <span class="info" data-type="tabName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="tabNameProperty">
                                                                </select>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>@Localizer["Group"]</label>
                                                                    <span class="info" data-type="groupName"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <select id="groupName"></select>
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>@Localizer["Width (in %)"]</label>
                                                                    <span class="info" data-type="width"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <input id="width" type="number" title="numeric" class="numeric" value="" min="0" max="100" step="1" autocomplete="off" />
                                                            </div>

                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>@Localizer["Height (in pixels)"]</label>
                                                                    <span class="info" data-type="height"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <input id="height" type="number" title="numeric" class="numeric" value="" max="65535" min="0" step="1" autocomplete="off" />
                                                            </div>
                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>@Localizer["Language code"]</label>
                                                                    <span class="info" data-type="languageCode"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="langCode" type="text" class="textField k-input" maxlength="5" autocomplete="off" />
                                                                </span>
                                                            </div>
                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>@Localizer["Hotkey (access key)"]</label>
                                                                    <span class="info" data-type="accessKey"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="accessKey" type="text" class="textField k-input" maxlength="1" autocomplete="off" />
                                                                </span>
                                                            </div>
                                                            <div class="item" style="width: 25%;">
                                                                <h4>
                                                                    <label>@Localizer["Label style"]</label>
                                                                    <span class="info" data-type="labelStyle"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <select id="labelStyle"></select>
                                                                </span>
                                                            </div>
                                                            <div class="item" id="labelWidthContainer" style="width: 25%;">
                                                                <h4>
                                                                    <label>@Localizer["Label width"]</label>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <select id="labelWidth"></select>
                                                                </span>
                                                            </div>
                                                            <div class="item" style="width: 50%;">
                                                                &nbsp;
                                                            </div>
                                                            <div class="item" style="width: 50%;" data-visible="Input NumericInput SecureInput TextBox GpsLocation">
                                                                <h4>
                                                                    <label>@Localizer["Regex validation"]</label>
                                                                    <span class="info" data-type="regexValidation"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="regexValidation" type="text" class="textField k-input" max="255" autocomplete="off" />
                                                                </span>
                                                            </div>
                                                            <div class="item" style="width: 50%;" data-visible="Input NumericInput SecureInput TextBox GpsLocation">
                                                                <h4>
                                                                    <label>@Localizer["Regex for Path"]</label>
                                                                    <span class="info" data-type="visibilityPathRegex"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="visibilityPathRegex" type="text" class="textField k-input" max="255" autocomplete="off" />
                                                                </span>
                                                            </div>
                                                            <div class="item" data-invisible="FileUpload ImageUpload ColorPicker DataSelector SubEntitiesGrid">
                                                                <div class="toggle">
                                                                    <input id="visible-in-table" type="checkbox" class="noForm">
                                                                    <label class="checkbox" for="visible-in-table">
                                                                        <span>@Localizer["Visible by default in summary tables"]</span>
                                                                    </label>
                                                                    <div class="togglePanel">
                                                                        <div class="item" style="width: 50%;">
                                                                            <h4>
                                                                                <label>@Localizer["Width in table (in pixels)"]</label>
                                                                                <span class="info" data-type="overviewWidth"><ins class="icon-info-full"></ins></span>
                                                                            </h4>
                                                                            <input id="widthIfVisible" type="number" class="numeric" min="0" step="1" max="65535" autocomplete="off" />
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="item" data-invisible="FileUpload ImageUpload DataSelector SubEntitiesGrid">
                                                                <div class="toggle">
                                                                    <input id="enableAggregation" type="checkbox" class="noForm">
                                                                    <label class="checkbox" for="enableAggregation">
                                                                        <span>@Localizer["Enable aggregation"]</span>
                                                                    </label>
                                                                    <div class="togglePanel">
                                                                        <div class="item" style="width: 100%;">
                                                                            <h4>
                                                                                <label>@Localizer["Aggregation-settings"]</label>
                                                                                <span class="info" data-type="aggregateOptions"><ins class="icon-info-full"></ins></span>
                                                                            </h4>
                                                                            <textarea id="aggregateOptions"></textarea>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="item">
                                                                <span class="inline">
                                                                    <label class="checkbox" data-invisible="RadioButton CheckBox Empty">
                                                                        <input id="mandatory" type="checkbox">
                                                                        <span>@Localizer["Required field"]</span>
                                                                    </label>
                                                                    <label class="checkbox" data-invisible="RadioButton CheckBox FileUpload ImageUpload ColorPicker DataSelector Empty">
                                                                        <input id="readonly" type="checkbox">
                                                                        <span>@Localizer["Read-only field"]</span>
                                                                    </label>
                                                                    <label class="checkbox" data-visible="Input">
                                                                        <input id="seofriendly" type="checkbox">
                                                                        <span>@Localizer["Save value of this field also as search engine friendly text"]</span>
                                                                    </label>
                                                                    <label class="checkbox" data-visible="Combobox">
                                                                        <input id="useDropDownList" type="checkbox">
                                                                        <span>@Localizer["Use combobox as dropdown"]</span>
                                                                    </label>
                                                                    <label class="checkbox" data-show-for-panel="panel2">
                                                                        <input id="searchInTitle" type="checkbox">
                                                                        <span>@Localizer["Search in title"]</span>
                                                                    </label>
                                                                    <label class="checkbox" data-show-for-panel="panel2">
                                                                        <input id="searchEverywhere" type="checkbox">
                                                                        <span>@Localizer["Show all items, instead of only linked items"]</span>
                                                                    </label>
                                                                    <label class="checkbox" data-visible="Input SecureInput TextBox RadioButton CheckBox ComboBox MultiSelect NumericInput DateTimePicker ColorPicker">
                                                                        <input id="saveOnChange" type="checkbox">
                                                                        <span>@Localizer["Automatically save item when the value of this field is changed"]</span>
                                                                    </label>
                                                                </span>
                                                            </div>

                                                            <div class="item tagList" data-show-for-panel="panel2">
                                                                <h4>
                                                                    <label>@Localizer["Search in fields"]</label>
                                                                    <span class="info" data-type="searchFields"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <select id="searchFields"></select>
                                                                </span>
                                                            </div>

                                                            <div class="item" data-visible="TextBox">
                                                                <h4>
                                                                    <label>@Localizer["Textbox type"]</label>
                                                                    <span class="info" data-type="textboxType"><ins class="icon-info-full"></ins></span>
                                                                </h4>
                                                                <span class="k-widget k-input k-state-default">
                                                                    <input id="textboxTypeDropDown" />
                                                                </span>
                                                            </div>

                                                            <div class="item" data-visible="Input SecureInput TextBox GpsLocation">
